{
  "name": "website-starter",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "npm run temp:setTypeToModule && node --experimental-json-modules --loader ts-node/esm ./scripts/dev/run.ts",
    "build": "next build",
    "serve": "next start",
    "test:eslint": "eslint ./ --fix",
    "test:ts-types": "tsc --noemit",
    "prettify": "node --loader  ts-node/esm ./devtools/prettier/scripts/runPrettier/runPrettier.ts",
    "helpers:generate-logo-assets": "npm run temp:setTypeToModule && node --experimental-json-modules --loader ts-node/esm ./devtools/helpers/assets/generateLogoAssets/run.ts",
    "helpers:create-icon": "npm run temp:setTypeToModule && node --experimental-json-modules --loader ts-node/esm ./devtools/helpers/createIcon/run.ts",
    "helpers:capture-page-screenshots": "npm run temp:setTypeToModule && node --loader  ts-node/esm ./scripts/assets/capturePageScreenshots/run.ts",
    "prepare": "npx @arkweid/lefthook install",
    "temp:setTypeToModule": "node ./devtools/utils/pkgJson/setTypeToModule.cjs",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.3.1",
    "emotion-theming": "^11.0.0",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "next-sitemap": "^1.9.9",
    "@svgr/core": "^6.2.1",
    "edit-json-file": "^1.7.0",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "capture-website": "^2.3.0",
    "chalk": "^4.1.2",
    "clipboardy": "^3.0.0",
    "command-line-args": "^5.2.0",
    "concurrently": "^7.0.0",
    "enquirer": "^2.3.6",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.9",
    "fs-extra": "^10.0.0",
    "ink": "^3.2.0",
    "internal-ip": "^7.0.0",
    "kill-port-process": "^3.0.1",
    "node-emoji": "^1.11.0",
    "ora": "^6.0.1",
    "prettier": "^2.5.1",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.29.3",
    "svgo": "^2.8.0",
    "tcp-port-used": "^1.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
